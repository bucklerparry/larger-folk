  <anatomies Load="Merge">
    <anatomy Name="FloatingHand">
        <part Type="Starched Cuff" Laterality="Left">
         <part Type="Hand" Laterality="Left" SupportsDependent="Hands" />
        </part>
        <part Type="Missile Weapon" Laterality="Left" />
        <part Type="Hands" DependsOn="Hands" />
    </anatomy>

    <!-- NPC-specific -->
    <anatomy Name="Humanoid" >
      <part Type="Head">
        <part Type="Face" />
      </part>
      <part Type="Back" />
      <part Type="Arm" Laterality="Right">
        <part Type="Hand" Laterality="Right" SupportsDependent="Hands" />
      </part>
      <part Type="Arm" Laterality="Left">
        <part Type="Hand" Laterality="Left" SupportsDependent="Hands" />
      </part>
      <part Type="Missile Weapon" Laterality="Right" />
      <part Type="Missile Weapon" Laterality="Left" />
      <part Type="Hands" DependsOn="Hands" />
      <part Type="Feet" />
    </anatomy>

     <anatomy Name="Quadruped" Load="Replace">
      <part Type="Head">
        <part Type="Face" />
      </part>
      <part Type="Back" />
      <part Type="Haunches" />
      <part Type="Foot" Laterality="Right Fore" SupportsDependent="Forefeet" />
      <part Type="Foot" Laterality="Left Fore" SupportsDependent="Forefeet" />
      <part Type="Foot" Laterality="Right Hind" SupportsDependent="Hindfeet" />
      <part Type="Foot" Laterality="Left Hind" SupportsDependent="Hindfeet" />
      <part Type="Missile Weapon" Laterality="Right" />
      <part Type="Missile Weapon" Laterality="Left" />
      <part Type="Feet" Laterality="Fore" DependsOn="Forefeet" Mobility="0" />
      <part Type="Feet" Laterality="Hind" DependsOn="Hindfeet" Mobility="0" />
      <part Type="Tail" />
    </anatomy>
